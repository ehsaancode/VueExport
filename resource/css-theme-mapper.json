{
  "dimension": {
    "width": "w-[var(variableName)]",

    "minWidth": "min-w-[var(variableName)]",

    "maxWidth": "max-w-[var(variableName)]",
    
    "height": "h-[var(variableName)]",

    "minHeight": "min-h-[var(variableName)]",

    "maxHeight": "max-h-[var(variableName)]",

    "overflow": {
      "overflowType": {
        "overflow": "overflow-[var(variableName)]"
      },
      "scroll": {
        "overflowX": {
          "overflow-x": "overflow-x-[var(variableName)]"
        },
        "overflowY": {
          "overflow-y": "overflow-y-[var(variableName)]"
        }
      }
    }
  },
  "spacing": {
    "padding": {
      "top":  "pt-[var(variableName)]",

      "right": "pr-[var(variableName)]",

      "bottom": "pb-[var(variableName)]",

      "left": "pl-[var(variableName)]"
    },
    "margin": {
      "top": "mt-[var(variableName)]",

      "right": "mr-[var(variableName)]",

      "bottom": "mb-[var(variableName)]",

      "left": "ml-[var(variableName)]"
    }
  },
  "position": {
    "positionType": {
      "positionType": "[position:var(variableName)]"
    },
    "positionValue": {
      "top": "top-[var(variableName)]",
      "right": "right-[var(variableName)]",
      "bottom": "bottom-[var(variableName)]",
      "left": "left-[var(variableName)]"
    }
  },
  "boxDecoration": {
    "borderStyle": {
      "all": {
        "solid": "border-solid",
        "dashed": "border-dashed",
        "dotted": "border-dotted",
        "double": "border-double",
        "none": "border-none"
      },
      "top": {
        "solid": "[border-top-style:solid]",
        "dashed": "[border-top-style:dashed]",
        "dotted": "[border-top-style:dotted]",
        "double": "[border-top-style:double]",
        "none": "[border-top-style:none]"
      },
      "right": {
        "solid": "[border-right-style:solid]",
        "dashed": "[border-right-style:dashed]",
        "dotted": "[border-right-style:dotted]",
        "double": "[border-right-style:double]",
        "none": "[border-right-style:none]"
      },
      "bottom": {
        "solid": "[border-bottom-style:solid]",
        "dashed": "[border-bottom-style:dashed]",
        "dotted": "[border-bottom-style:dotted]",
        "double": "[border-bottom-style:double]",
        "none": "[border-bottom-style:none]"
      },
      "left": {
        "solid": "[border-left-style:solid]",
        "dashed": "[border-left-style:dashed]",
        "dotted": "[border-left-style:dotted]",
        "double": "[border-left-style:double]",
        "none": "[border-left-style:none]"
      }
    },
    "borderWidth": {
      "all": "border-[var(variableName)]",

      "top": "border-t-[var(variableName)]",

      "right": "border-r-[var(variableName)]",

      "bottom":"border-b-[var(variableName)]",

      "left": "border-l-[var(variableName)]"
    },
    "borderColor": {
      "all": "border-[var(variableName)]",

      "top": "border-t-[var(variableName)]",

      "right": "border-r-[var(variableName)]",

      "bottom":"border-b-[var(variableName)]",

      "left": "border-l-[var(variableName)]"
    },
    "borderRadius": {
      "all": "rounded-[var(variableName)]",
      
      "topLeft": "rounded-tl-[var(variableName)]",

      "topRight": "rounded-tr-[var(variableName)]",

      "bottomLeft": "rounded-bl-[var(variableName)]",

      "bottomRight": "rounded-br-[var(variableName)]"
    }
  },
  "typography": {
    "text": {
      "fontFamily": "[font-family:var(variableName)]",

      "fontSize": "[font-size:var(variableName)]",

      "fontWeight": "[font-weight:var(variableName)]",

      "textColor": "text-[var(variableName)]",

      "lineHeight":  "leading-[var(variableName)]",

      "letterSpacing": "tracking-[var(variableName)]",

      "textAlign": {
        "textAlign:left;": "text-left",
        "textAlign:align_left;": "text-left",
        "textAlign:center;": "text-center",
        "textAlign:right;": "text-right",
        "textAlign:subject;": "text-right",
        "textAlign:justify;": "text-justify",
        "textAlign:start;": "text-start",
        "textAlign:end;": "text-end"
      },
      "textDecoration": {
        "line": {
          "line:underline;": "underline",
          "line:overline;": "overline",
          "line:lineThrough;": "line-through",
          "line:noUnderline;": "no-underline",
          "custom": "text-decoration-[{v}]"
        },
        "style": {
          "style:solid;": "decoration-solid",
          "style:double;": "decoration-double",
          "style:dotted;": "decoration-dotted",
          "style:dashed;": "decoration-dashed",
          "style:Wavy;": "decoration-wavy"
        },
        "color": {
          "custom": "decoration-[{v}{u}]"
        },
        "thick": {
          "custom": "decoration-[{v}{u}]"
        },
        "offset": {
          "custom": "underline-offset-[{v}{u}]"
        },
        
        "strokeWidth": "stroke-[var(variableName)]", 

        "strokeColor": {
          "custom": "strokeColor-[{v}]"
        }
      },
      "more": {
        "fontStyle": {
          "fontStyle:italicize;": "italic",
          "fontStyle:normal;": "not-italic",
          "custom": "font-style-[{v}]"
        },
        "fontVariant": {
          "fontVariant:normal;": "normal-case",
          "fontVariant:uppercase;": "uppeercase",
          "fontVariant:lowercase;": "lowercase",
          "fontVariant:capitalize;": "capitalize",
          "fontVariant:camelcase;": "capitalize"
        },

        "direction": {
          "direction:rightToLeft;": "direction-rtl",
          "direction:leftToRight;": "direction-ltr"
        },

        "wordBreak": {
          "wordBreak:normal;": "break-normal",
          "wordBreak:all;": "break-all",
          "wordBreak:keep;": "break-keep"
        }
      },
      "placeHolder": {
        "value": {
          "custom": "Lorem Ipsum"
        }
      }
    }
  },
  "cursor": {
    "type": {
      "type:auto;": "cursor-auto",
      "type:default;": "cursor-default",
      "type:pointer;": "cursor-pointer",
      "type:wait;": "cursor-wait",
